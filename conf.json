{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["closure", "jsdoc"]
  },
  "source": {
    "include": "./lib/jscc",
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "recurse": true,
    "destination": "html-documentation",
    "package": "./package.json",
    "readme": "./README.md"
  }
}