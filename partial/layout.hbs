<!doctype html>
<html>
<!-- This file is automatically generated.  Please edit the templates (e.g., pages/*.hbs, partial/*.hbs) instead
     of editing the .html file directly. -->
{{>head title=title}}
<body>
{{>nav}}
{{#if documentation}}
    <div class="container-fluid">
        <div class="row">
            <div class="documentation-table-of-contents">
              {{>documentation-table-of-contents documentation=documentation}}
            </div>
            <div class="documentation-main">
              {{>documentation-paging}}
              {{> @partial-block }}
              {{>documentation-paging}}
            </div>
        </div>
        <div class="row">
            <div class="col-xs-12">
              {{>footer}}
            </div>
        </div>
    </div>
{{else}}
  {{#if demo}}
      <div class="container-fluid">
        {{> @partial-block }}
        {{>footer}}
      </div>
  {{else}}
      <div class="container">
        {{> @partial-block }}
        {{>footer}}
      </div>
  {{/if}}
{{/if}}
{{#if demo}}
    <script src="{{resolve "javascripts/demo-built.js"}}"></script>
{{else}}
    <script src="{{resolve "javascripts/main-built.js"}}"></script>
{{/if}}
</body>
</html>
